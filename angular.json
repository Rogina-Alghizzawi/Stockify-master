{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Stocify": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/stocify",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/ngx-toastr/toastr.css",

              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "src/assets/AdminDesign/vendor/fontawesome-free/css/all.min.css",
              "src/assets/AdminDesign/css/sb-admin-2.min.css"
            ],
            "scripts": [
              "src/assets/AdminDesign/vendor/jquery/jquery.min.js",
              "src/assets/AdminDesign/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/AdminDesign/vendor/jquery-easing/jquery.easing.min.js",
              "src/assets/AdminDesign/js/sb-admin-2.min.js",
              "src/assets/AdminDesign/vendor/chart.js/Chart.min.js",
              "src/assets/AdminDesign/js/demo/chart-area-demo.js",
              "src/assets/AdminDesign/js/demo/chart-pie-demo.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Stocify:build:production"
            },
            "development": {
              "buildTarget": "Stocify:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "Stocify:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "9548ce5c-3ac3-4554-8dee-dd680e6d01a4"
  }
}
